{
  "name": "drone-delivery-system",
  "version": "1.0.0",
  "description": "A REST API for managing drone medication deliveries",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "seed": "ts-node src/config/seedData.ts"
  },
  "keywords": ["drone", "delivery", "medication", "api", "rest"],
  "author": "Blusalt Engineering",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/jest": "^29.5.3",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}